		} else if($("#addto").find("option:selected").attr("value") == "newlist"){
			<!-- TMPL_IF NAME="loggedinusername" -->if (vShelfAdd()) {
			Dopop('/cgi-bin/koha/opac-addbybiblionumber.pl?newshelf=1&' + vShelfAdd());
