});

function highlightOff() {
    $("td").removeHighlight();
    $(".highlight_toggle").toggle();
}
function highlightOn() {
    var x;
    for (x in q_array) {
        $("td").highlight(q_array[x]);
    }
    $(".highlight_toggle").toggle();
}
